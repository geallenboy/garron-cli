{
  "name": "@garron/i18n-cli",
  "version": "1.18.1",
  "description": "garron i18n 是一款使用 ChatGPT 自动化 i18n 的 CLI 流程工具",
  "keywords": [
    "ai",
    "i18n",
    "openai",
    "gpt",
    "langchain"
  ],
  "homepage": "https://github.com/geallenboy/garron-cli/tree/master/packages/garron-i18n",
  "bugs": {
    "url": "https://github.com/geallenboy/garron-cli/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geallenboy/garron-cli.git"
  },
  "license": "MIT",
  "author": "gejialun88@gmail.com",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "@": "./src"
  },
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": {
    "garron-i18n": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run type-check && pkgroll --minify -p tsconfig.prod.json --env.NODE_ENV=production && npm run shebang",
    "dev": "pkgroll -p tsconfig.prod.json --env.NODE_ENV=development --watch",
    "link": "npm run build && npm link -f",
    "shebang": "garron-shebang -t ./dist/cli.js",
    "start": "node ./dist/cli.js",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@inkjs/ui": "^1",
    "@langchain/core": "latest",
    "@langchain/openai": "latest",
    "@garron/cli-ui": "file:../garron-cli-ui",
    "chalk": "^5",
    "commander": "^11",
    "conf": "^12",
    "consola": "^3",
    "cosmiconfig": "^9",
    "dotenv": "^16",
    "fast-deep-equal": "^3",
    "glob": "^10",
    "gpt-tokenizer": "^2",
    "gray-matter": "^4",
    "ink": "^4.2",
    "json-stable-stringify": "^1",
    "just-diff": "^6",
    "langchain": "latest",
    "lodash-es": "^4",
    "p-map": "^7",
    "pangu": "^4",
    "react": "^18",
    "remark-frontmatter": "^4",
    "remark-gfm": "^3",
    "remark-parse": "^10",
    "remark-stringify": "^10",
    "swr": "^2",
    "unified": "^11",
    "unist-util-visit": "^5",
    "update-notifier": "^7",
    "zustand": "^4"
  },
  "devDependencies": {
    "@types/json-stable-stringify": "^1"
  },
  "peerDependencies": {
    "ink": ">=4",
    "react": ">=18"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
